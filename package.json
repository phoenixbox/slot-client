    "vinyl-transform": "*",
    "watchify": "^3.2.1",
    "websocket": "^1.0.18"
  },
  "scripts": {
    "dev": "gulp dev",
    "build": "browserify app -d | exorcist -b './app' public/js/coffeeslots.js.map > public/js/coffeeslots.js",
    "build-less": "gulp less",
    "build-production": "npm run build-production-js && npm run build-production-css",
    "build-production-css": "gulp css",
    "build-production-js": "NODE_ENV=production browserify app | uglifyjs -cm > public/js/coffeeslots.min.js",
    "build-test": "NODE_ENV=test browserify test/index.js -t rewireify -t babelify -t envify -d | exorcist -b './app' test/test-bundle.js.map > test/test-bundle.js",
    "start": "node server.js",
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start --browsers Firefox --single-run",
    "unit-test": "mocha ./test/unit --compilers js:compiler.js --recursive",
    "watch-unit-test": "mocha ./test/unit --watch --compilers js:compiler.js --recursive",
    "watch-test": "NODE_ENV=test ./node_modules/karma/bin/karma start",
    "watchify": "watchify app -d -v -o 'exorcist -b './app' public/js/coffeeslots.js.map > public/js/coffeeslots.js'",
    "watchify-test": "NODE_ENV=test watchify test/index.js -o 'exorcist -b './app' test/test-bundle.js.map > test/test-bundle.js' -t rewireify -t babelify -t envify -dv"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "main": "server.js",
  "keywords": [
    "coffeeslots"
  ]
}
