    "moment-timezone": "^0.3.1",
    "node-notifier": "^4.2.3",
    "path": "^0.11.14",
    "react": "*",
    "react-inlinesvg": "^0.4.2",
    "react-router": "*",
    "react-spinner": "^0.2.2",
    "require-dir": "^0.3.0",
    "rewireify": "^0.2.1",
    "s3": "^4.4.0",
    "sinon": "^1.14.1",
    "superagent": "^1.3.0",
    "superagent-bluebird-promise": "^2.0.2",
    "tcomb-form": "^0.7.0",
