    "lodash": "^3.8.0",
    "moment": "^2.10.2",
    "pg": "^4.3.0",
    "qs": "^4.0.0",
    "react-d3": "^0.3.1",
    "request": "^2.55.0",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "async": "^1.4.2",
    "aws-sdk": "^2.1.42",
    "babel": "^5.6.23",
    "babelify": "^5.0.5",
    "bootstrap": "^3.3.4",
    "browser-sync": "^2.7.6",
    "browserify": "^9.0.0",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "classnames": "^2.1.3",
    "connect": "^3.3.5",
    "csso": "^1.3.11",
